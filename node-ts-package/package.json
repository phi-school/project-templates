	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/main.js",
	"module": "./dist/main.mjs",
	"types": "./dist/main.d.ts",
	"exports": {
		"require": {
			"types": "./dist/main.d.ts",
			"default": "./dist/main.js"
		},
		"import": {
			"types": "./dist/main.d.mts",
			"default": "./dist/main.mjs"
		}
	},
	"scripts": {
		"build": "tsup",
		"format:check": "prettier --check \"**/*.{js,json,md,ts}\"",
		"format": "prettier --ignore-path .gitignore --write '**/*.{js,json,md,ts}'",
		"lint": "eslint --cache --fix --ignore-path .gitignore .",
		"prepack": "pnpm build && clean-pkg-json",
		"prepare": "husky install",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"type-check": "tsc"
	},
	"engines": {
		"engines": {
			"node": "^20.0.0"
		}
	},
	"devDependencies": {
		"@commitlint/cli": "^18.0.0",
		"@commitlint/config-conventional": "^18.0.0",
		"@commitlint/load": "^18.2.0",
		"@phi.school/eslint-config": "^0.3.0",
		"@phi.school/prettier-config": "^0.2.0",
		"@types/node": "^20.8.2",
		"@vitest/ui": "^1.1.1",
		"clean-pkg-json": "^1.2.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.50.0",
		"husky": "^9.0.6",
		"lint-staged": "^15.0.1",
		"prettier": "^3.0.3",
		"tsup": "^8.0.1",
		"typescript": "^5.2.2",
		"valibot": "^0.29.0",
		"vite-tsconfig-paths": "^4.2.1",
		"vitest": "^1.1.1"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"eslintConfig": {
		"extends": "@phi.school/eslint-config"
	},
	"lint-staged": {
		"*.{js,ts}": "eslint --cache --fix",
		"*.{js,json,md,ts}": "prettier --write"
	},
	"prettier": "@phi.school/prettier-config"
}
